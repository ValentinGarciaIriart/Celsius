{
    "steps": [
      {
        "name": "gcr.io/cloud-builders/secret-manager",
        "args": [
          "access",
          "projects/395145178509/secrets/dckusr/versions/latest",
          "--format=json"
        ],
        "env": [
          "DCKUSR=$(echo \"$.data.username\" | jq -r .)"
        ]
      },
      {
        "name": "gcr.io/cloud-builders/secret-manager",
        "args": [
          "access",
          "projects/395145178509/secrets/dckpsw/versions/latest",
          "--format=json"
        ],
        "env": [
          "DCKPSW=$(echo \"$.data.password\" | jq -r .)"
        ]
      },
      {
        "name": "gcr.io/cloud-builders/docker",
        "entrypoint": "bash",
        "args": [
          "-c",
          "echo \"${DCKPSW}\" | docker login --username=\"${DCKUSR}\" --password-stdin"
        ]
      }
    ]
  }
  